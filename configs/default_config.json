{
  "agent_type": "BaseAgent",
  "date_range": {
    "init_date": "2025-10-01",
    "end_date": "2025-10-21"
  },
  "models": [
    {
      "name": "gemini-pro-2.5",
      "basemodel": "google/gemini-2.5-pro",
      "signature": "gemini-pro-2.5",
      "enabled": true,
      "backend": "openai_api",
      "openai_base_url": "${GEMINI_API_BASE}",
      "openai_api_key": "${GEMINI_API_KEY}"
    },
    {
      "name": "groq-kimi-k2",
      "basemodel": "moonshotai/kimi-k2-instruct",
      "signature": "kimi-k2-instruct",
      "enabled": true,
      "backend": "openai_api",
      "openai_base_url": "${GROQ_API_BASE}",
      "openai_api_key": "${GROQ_API_KEY}"
    },
    {
      "name": "minimax-m2-cloud",
      "basemodel": "minimax-m2",
      "signature": "minimax-m2-cloud",
      "enabled": true,
      "backend": "ollama_cloud",
      "server_url": "${OLLAMA_API_BASE}",
      "api_key": "${OLLAMA_API_KEY}"
    }
  ],
  "agent_config": {
    "max_steps": 30,
    "max_retries": 3,
    "base_delay": 1.0,
    "initial_cash": 10000.0
  },
  "log_config": {
    "log_path": "./data/agent_data"
  }
}

